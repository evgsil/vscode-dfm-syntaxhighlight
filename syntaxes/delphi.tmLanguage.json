{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Delphi",
	"fileTypes": [
		"source.dfm"
	],
	"firstLineMatch": "^object\\s+\\w{1}\\s*:\\s*\\w$",
	"foldingStartMarker": "^object\\s+\\w{1}\\s*:\\s*\\w$",
	"foldingStopMarker": "^end$",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#constants"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.object.dfm",
				"match": "\\b(object|end)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.single.dfm",
			"begin": "'",
			"end": "'",
			"patterns": [
				
			]
		},
		"constants": {
			"patterns": [{
				"name": "constant.numeric.dfm",
				"match": "\\W\\d+"
			},
			{
				"name": "constant.language.dfm",
				"match": "([Tt]rue|[Ff]alse)"
			}
		]
		}
	},
	"scopeName": "source.dfm"
}